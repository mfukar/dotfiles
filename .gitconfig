    name = Michael Foukarakis
    #email = mfukar@census-labs.com
    email = michael.foukarakis@ioactive.com
[http]
    sslVerify = false
[gui]
[color]
    diff = auto
    status = auto
    branch = auto
[core]
    pager = less
    editor = vim
[core "whitespace"]
    tabwidth = 4
[diff]
    tool = vimdiff
[alias]
    history = log --color --graph --pretty=format:'%Cred%h%Creset |%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset [%C(bold blue)%an%Creset]'
[push]
    default = matching
